{
  "search-result-layout.mobile": {
    "children": [
      "flex-layout.row#search-result--mobile"
    ]
  },

  "flex-layout.row#search-result--mobile": {
    "title": "Search Result Row",
    "children": [
      "flex-layout.col#search-result--mobile"
    ],
    "props": {
      "blockClass": "search-result--mobile",
      "fullWidth": true
    }
  },

  "flex-layout.col#search-result--mobile": {
    "title": "Search Result Col",
    "children": [
      "rich-text#search--mobile",
      "search-bar#search-result",
      "search-content#search-result"
    ],
    "props": {
      "blockClass": "search-result--mobile"
    }
  },
  "rich-text#search--mobile": {
    "props": {
      "text": "BUSCAR",
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "blockClass": "search-text--mobile"
    }
  }
}
