    }
  },

  "flex-layout.col#card__two--phone": {
    "children": [
      "image#card__two"
    ],
    "props": {
      "horizontalAlign": "center"
    }
  },

  "image#card__one": {
    "props": {
      "src": "assets/img/products/woman/white-top.jpg"
    }
  },
  "image#card__two": {
    "props": {
      "src": "assets/img/products/woman/black-top.jpg"
    }
  },

  "flex-layout.row#releases-section__explore-container--phone": {
    "children": [
      "flex-layout.row#explore__card--men",
      "flex-layout.row#explore__card--woman"
